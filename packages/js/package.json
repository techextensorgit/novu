{
  "name": "@novu/js",
  "version": "2.0.0-canary.0",
  "repository": "https://github.com/novuhq/novu",
  "description": "Novu's JavaScript SDK for building custom inbox notification experiences",
  "author": "",
  "license": "ISC",
  "main": "dist/cjs/index.cjs",
  "module": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.cjs",
      "import": "./dist/esm/index.mjs",
      "default": "./dist/esm/index.mjs",
      "types": "./dist/types/index.d.ts"
    },
    "./ui": {
      "require": "./dist/cjs/ui/index.cjs",
      "import": "./dist/esm/ui/index.mjs",
      "default": "./dist/esm/ui/index.mjs",
      "types": "./dist/types/ui/index.d.ts"
    }
  },
  "files": [
    "dist/cjs",
    "dist/esm",
    "dist/types"
  ],
  "sideEffects": false,
  "private": true,
  "scripts": {
    "clean": "rimraf ./dist",
    "start": "pnpm run build -- --watch --sourcemap",
    "pre:build": "cp ./src/ui/index.css ./src/ui/index.directcss",
    "build": "pnpm run clean && pnpm run pre:build && tsup && pnpm run post:build",
    "build:declarations": "tsc -p tsconfig.declarations.json",
    "build:umd": "webpack --config webpack.config.cjs",
    "build:watch": "pnpm run pre:build && tsup --watch && pnpm run post:build",
    "post:build": "rm ./src/ui/index.directcss && node scripts/size-limit.mjs",
    "lint": "eslint --ext .ts,.tsx src",
    "test": "jest"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/jest": "^29.2.3",
    "@types/node": "^20.14.10",
    "autoprefixer": "^10.4.0",
    "bytes-iec": "^3.1.1",
    "chalk": "^5.3.0",
    "compression-webpack-plugin": "^10.0.0",
    "esbuild-plugin-compress": "^1.0.1",
    "esbuild-plugin-inline-import": "^1.0.4",
    "esbuild-plugin-solid": "^0.6.0",
    "eslint-plugin-local-rules": "^3.0.2",
    "jest": "^29.3.1",
    "postcss": "^8.4.38",
    "postcss-load-config": "^6.0.1",
    "postcss-prefix-selector": "^1.16.1",
    "postcss-preset-env": "^9.5.14",
    "solid-devtools": "^0.29.2",
    "tailwindcss": "^3.4.4",
    "terser-webpack-plugin": "^5.3.9",
    "tiny-glob": "^0.2.9",
    "ts-jest": "^29.0.3",
    "ts-loader": "~9.4.0",
    "tsup": "^8.1.0",
    "tsup-preset-solid": "^2.2.0",
    "typescript": "4.9.5",
    "webpack": "^5.74.0",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.6.7",
    "@novu/client": "workspace:*",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "mitt": "^3.0.1",
    "socket.io-client": "4.7.2",
    "solid-floating-ui": "^0.3.1",
    "solid-js": "^1.8.11",
    "tailwind-merge": "^2.4.0"
  }
}
